generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model memes {
  id          Int    @id @default(autoincrement())
  url         String
  title       String @unique
  description String
  userId      Int
}
model users {
  id          Int    @id @default(autoincrement())
  name        String
  email       String @unique
  password    String
}
